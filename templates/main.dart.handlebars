// generated file, do not edit

part of '{{file_name}}';

{{#each classes}}
// ------------------------------------------------------------
{{#if is_mixin}}
mixin {{class_name}}CopyWith {
  {{#each fields}}
  {{type_str_nullable}} get {{name_str}};
  {{/each}}

  {{class_name}} copyWith({
    {{#each fields}}
    {{type_str_wrapped_nullable_value}} {{name_str}},
    {{/each}}
  }) => {{class_name}}(
    {{#each fields}}
    {{name_str}}: {{name_str}} {{#if is_nullable}} / {{else}} ?? {{/if}} this.{{name_str}},
    {{/each}}
  );
}
{{else}}
extension {{class_name}}CopyWith on {{class_name}} {
  {{class_name}} Function({
    {{#each fields}}
    {{type_str_nullable}} {{name_str}},
    {{/each}}
  }) get copyWith => ({
        {{#each fields}}
        Object? {{name_str}} = Never,
        {{/each}}
      }) =>
          {{class_name}}(
            {{#each fields}}
            {{name_str}}: {{name_str}}._or(this.{{name_str}}),
            {{/each}}
          );
}
{{/if}}
{{/each}}
// ------------------------------------------------------------

extension on Object? {
  // T? _as<T>() => this is T ? this as T : null;
  T _or<T>(T other) => this is T ? this as T : other;
}
